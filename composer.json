{
    "name": "colbydude/ffxiv-progression-checker",
    "description": "Simple interface for checking a character's raid progression for FFXIV.",
    "homepage": "http://ffxiv.voidteam.net/progression",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Colby Terry",
            "email": "colbydude@voidteam.net",
            "homepage": "https://colbydude.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "xivapi/xivapi-php",
                "version": "1.2.3",
                "source": {
                    "url": "https://github.com/Colbydude/xivapi-php",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "XIVAPI\\": "src/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "xivapi/xivapi-php": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
