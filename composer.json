{
    "name": "colbydude/ffxiv-progression-checker",
    "description": "Simple interface for checking a character's raid progression for FFXIV.",
    "homepage": "http://ffxiv.voidteam.net/progression",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Colby Terry",
            "email": "colbydude@voidteam.net",
            "homepage": "http://colbydude.com"
        }
    ],
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "viion/xivpads-lodestoneapi": "dev-master",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
